<div id="ticket">
  <h2><%= @ticket.title %></h2>
  <%= simple_format(@ticket.description) %>
</div>

<p>Created by <%= @ticket.user.email %>

<%= link_to "Edit Ticket", edit_project_ticket_path(@project, @ticket) %>
<%= link_to "Delete Ticket", [@project, @ticket], :method => "delete", :confirm => "Are you sure?" %>

